{
  "name": "api-tools",
  "version": "1.0.4",
  "description": "API Tools",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "test": "jest --ci --reporters=default --reporters=jest-Junit",
    "test:cover": "jest --coverage",
    "prebuild": "rm -rf dist",
    "build": "rollup --config --sourcemap"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeromeklam/api-tools.git"
  },
  "keywords": [
    "API",
    "tOOLS"
  ],
  "author": "jeromeklam@free.fr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeromeklam/api-tools/issues"
  },
  "homepage": "https://github.com/jeromeklam/api-tools#readme",
  "dependencies": {
    "@babel/runtime": "^7.20.13",
    "lodash": "^4.17.21",
    "loglevel": "^1.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@rollup/plugin-alias": "^4.0.3",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-image": "^3.0.2",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "jest": "^29.4.1",
    "jest-junit": "^15.0.0",
    "rollup": "^3.12.0",
    "rollup-jest": "^3.1.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-sourcemaps": "^0.6.3"
  },
  "engines": {
    "node": ">=16.0.0 || >=18.0.0"
  },
  "jest": {
    "transform": {
      "\\.js$": [
        "rollup-jest"
      ]
    }
  },
  "jest-junit": {
    "outputDirectory": "reports",
    "outputName": "jest-junit.xml",
    "ancestorSeparator": " â€º ",
    "uniqueOutputName": "false",
    "suiteNameTemplate": "api-tools",
    "classNameTemplate": "api-tools",
    "titleTemplate": "api-tools"
  }
